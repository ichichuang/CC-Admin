# 开发环境配置
NODE_ENV=development

# 开发服务器端口
VITE_PORT=5173

# 应用根路径
VITE_PUBLIC_PATH=/

# 是否启用 CDN
VITE_CDN=false

# 压缩方式 none | gzip | brotli | both
VITE_COMPRESSION=none

# 是否生成 sourcemap
VITE_BUILD_SOURCEMAP=true

# 是否启用构建分析
VITE_BUILD_ANALYZE=false

# 是否启用 legacy 浏览器支持
VITE_LEGACY=false

# API 基础路径
VITE_API_BASE_URL=http://localhost:3003

# 应用标题
VITE_APP_TITLE=Ant Design UI
