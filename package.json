{
  "name": "cc-admin",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=24.3.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:analyze": "vue-tsc --noEmit && vite build --mode analyze",
    "report": "npx vite-bundle-analyzer dist/stats.html",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit",
    "naming-check": "node scripts/naming-rules.js",
    "code-check": "pnpm type-check && pnpm lint && pnpm naming-check",
    "code-fix": "pnpm format && pnpm lint",
    "pre-commit": "pnpm code-check",
    "commit": "git-cz"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,sass,less,styl,json,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "alova": "^3.3.3",
    "crypto-js": "^4.2.0",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.4.0",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.17.0",
    "@iconify/json": "^2.2.353",
    "@tsconfig/node22": "^22.0.2",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.10.1",
    "@unocss/preset-attributify": "^66.3.2",
    "@unocss/preset-icons": "^66.3.2",
    "@unocss/preset-typography": "^66.3.2",
    "@unocss/preset-uno": "^66.3.2",
    "@unocss/reset": "^66.3.2",
    "@unocss/transformer-directives": "^66.3.2",
    "@unocss/transformer-variant-group": "^66.3.2",
    "@vitejs/plugin-legacy": "^7.0.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitejs/plugin-vue-jsx": "^5.0.0",
    "@vue/tsconfig": "^0.7.0",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dayjs": "^1.11.13",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.31.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.4.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.89.2",
    "terser": "^5.36.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.18.1",
    "unocss": "^66.3.2",
    "vite": "^7.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vue-tsc": "^2.2.10"
  }
}
